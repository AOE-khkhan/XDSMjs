{
  "name": "xdsmjs",
  "version": "0.1.0",
  "description": "XDSM diagram generator ",
  "main": "xdsm.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "lint": "eslint xdsm.js ./src/* || true",
    "build": "browserify . -o ./build/xdsm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OneraHub/XDSMjs.git"
  },
  "keywords": ["XDSM", "MDO"],
  "author": "Remi Lafage",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OneraHub/XDSMjs/issues"
  },
  "homepage": "https://github.com/OneraHub/XDSMjs#readme",
  "devDependencies": {
    "tape": "^4.5.1",
    "eslint": "^2.10.2",
    "browserify": "^13.0.1",
    "eslint-config-google": "^0.5.0"
  },
  "dependencies": {
    "d3": "^3.5.17"
  },
  "eslintConfig": {
    "extends": "google",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
        "linebreak-style": "off",        
        "require-jsdoc": "off"
    }
  } 
}
